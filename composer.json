{
    "name": "xabbuh/xapi-client-bundle",
    "description": "Symfony integration of the Experience API (xAPI) client",
    "keywords": ["xAPI", "Experience API", "Tin Can API", "client", "Symfony", "bundle"],
    "homepage": "https://github.com/xabbuh/XApiClientBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Christian Flothmann",
            "homepage": "https://github.com/xabbuh"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/xabbuh/serializer.git"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "jms/serializer": "dev-polymorphic-bugfix",
        "jms/serializer-bundle": "~0.9",
        "symfony/config": "~2.2",
        "symfony/dependency-injection": "~2.2",
        "symfony/http-kernel": "~2.2",
        "xabbuh/xapi-client": "~1.0@dev",
        "xabbuh/xapi-common": "~1.0@dev"
    },
    "autoload": {
        "psr-4": {
            "Xabbuh\\XApi\\ClientBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
